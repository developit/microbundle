{
  "extends": [
    "eslint-config-developit",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "brace-style": "off",
    "comma-dangle": ["warn", "always-multiline"],
    "indent": "off",
    "require-atomic-updates": "off"
  },
  "settings": {
    "react": {
      "version": "16.6"
    }
  },
  "ignorePatterns": [
    "dist"
  ]
}
